Proceso Producto
	
	Definir v Como Entero;
	Definir porcentaje Como Real;
	
	Escribir "Ingresa el valor del producto:";
	Leer v;
	
	
	Si v>= 50000 Entonces
		porcentaje= v*0.05;
		Escribir " Tendrá un descuento del 5% que es igual a: ", porcentaje ;
	SiNo
		Escribir " No tendrá un descuento ";
	Fin Si
	
	
FinProceso
