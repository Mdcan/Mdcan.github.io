Proceso Ganancia_Perdida
	
	Definir ingreso,gastos Como Real;
	
	Escribir "Ingrese el valor de ingreso:";
	Leer ingreso;
	Escribir "Ingrese el valor de gastos:";
	Leer gastos;
	
	
	Si ingreso>gastos Entonces
		Escribir " El ingreso de: " , ingreso , " es mayor al gasto de: " , gastos , " esto es ganancia ";
	SiNo
		Escribir " El ingreso de: " , ingreso , " es menor al gasto de: " , gastos , " esto es perdida ";
	Fin Si
	
	
FinProceso
