Algoritmo MasaCorporal
	
	Definir nombre Como Caracter;
	Definir peso,estatura,masa Como Real;
	
	Escribir "Digite su nombre:";
	Leer nombre;
	Escribir "Digite su peso (Kg):";
	Leer peso;
	Escribir "Digite su estatura:";
	Leer estatura;
	
	masa<-peso/(estatura*estatura);
	
	Escribir " Usted " nombre " tiene una peso de: " , peso , " una estatura de: ", estatura , " y su masa corporal es de: " , masa;
	
	
FinAlgoritmo

