Algoritmo Datos
	
	Definir nombre Como Caracter;
	Definir edad Como Entero;
	Definir peso Como Real;
	
	
	Escribir "Digite su nombre:";
	Leer nombre;
	Escribir "Digite su edad:";
	Leer edad;
	Escribir "Digite su peso:";
	Leer peso;
	
	
	
	Escribir " Los datos que usted a ingresado son los siguientes: ", nombre , " de " , edad , " años de edad  y con un peso de: ", peso;
	
	
	
FinAlgoritmo
